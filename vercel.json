{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/_next/static/(.*)", "dest": "/_next/static/$1" },
    { "src": "/_next/data/(.*)", "dest": "/_next/data/$1" },
    { "src": "/(.*)", "dest": "/" }
  ]
}
